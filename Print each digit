a = int(input('Please enter a number: '))
i = 10
f = 0
e = a

for d in range(1,a):
    
    e = int(e/10)
    f = f+1
    
    if(e==0):
        
        i=i**(f-1)
        break
        
for c in range(f):
    
    b = int(a/i)
    a = a%i
    i = i/10
    print(b)
    
    if(a==0):

        break
