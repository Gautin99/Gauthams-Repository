a = int(input('Please enter a number: '))
i = 10
f = 0
e = a
g = a
sum = 0

for d in range(1,a):
    
    e = int(e/10)
    f = f+1
    
    if(e==0):
        
        i=i**(f-1)
        break
        
for c in range(f):
    
    b = int(g/i)
    g = g%i
    i = i/10
    sum = sum + b**3
    
    if(g==0):

        break

if(sum==a):
    
    print('{} IS an Armstrong Number.'.format(a))

else:
    
    print('{} IS NOT an Armstrong Number.'.format(a))
